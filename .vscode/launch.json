    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
        },
        {
            "name": "Training",
            "type": "python",
            "request": "launch",
            "program": "training.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--model=LinearRegression",
                "--data_gen=Currin"
            ]
        },
        {
            "name": "Baseline Epidemiology Training",
            "type": "python",
            "request": "launch",
            "program": "baseline_epidemiology_training.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--model=GradientBoostingRegressor",
                "--seq_len=3",
                "--n_data=1000"
            ]
        },
        {
            "name": "Epidemiology Training",
            "type": "python",
            "request": "launch",
            "program": "epidemiology_training.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--multiplication_net=PDCLow",
                "--lag_size=1",
                "--n_degree=2",
                "--seq_len=2",
                "--epochs=5",
                "--loss_fn=WeightedMSELoss",
                "--n_data=1000"
            ]
        },
        {
            "name": "Models & Data",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceRoot}"}
        }
    ]
}